<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="OuinexDesktop.Views.ChartWindow"
        Title="ChartWindow"
		xmlns:vm="using:OuinexDesktop.ViewModels"
		 xmlns:ScottPlot="clr-namespace:ScottPlot.Avalonia;assembly=ScottPlot.Avalonia"
       
		x:DataType="vm:ChartViewModel">
	<Window.DataContext>
		<vm:ChartViewModel/>
	</Window.DataContext>
	<Grid RowDefinitions="auto,*">
		<Expander Header="Click To Expand">
			<TextBlock>Expanded</TextBlock>
		</Expander>
		<DockPanel Grid.Row="1">
			<ContentControl Content="{Binding MainChart}"/>
		</DockPanel>
	</Grid>
</Window>
