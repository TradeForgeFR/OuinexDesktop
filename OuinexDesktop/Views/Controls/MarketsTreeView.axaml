<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OuinexDesktop.Views.Controls.MarketsTreeView">
	<TabControl>
		<TabItem Header="Markets">
			<Grid RowDefinitions="auto,*">
				<AutoCompleteBox MinWidth="180"
								 Margin="0,3"
								 FontSize="10"
								 Watermark="Search ticker ..."/>
				<Panel Grid.Row="1">
					<TreeView>
						<TreeViewItem Header="Spot">
							<TreeViewItem Header="BTC/USDT" />
							<TreeViewItem Header="ETH/USDT" />
							<TreeViewItem Header="XRP/BTC" />
							<TreeViewItem Header="XRP/jEUR" />
							<TreeViewItem Header="Forex">
								<TreeViewItem Header="jEUR/jUSD" />
								<TreeViewItem Header="jCHF/jGBP" />
							</TreeViewItem>
						</TreeViewItem>
						<TreeViewItem Header="Margin assets">
							<TreeViewItem Header="jEUR/jUSD" />
							<TreeViewItem Header="jCHF/jGBP" />
						</TreeViewItem>
						<TreeViewItem Header="Futures">
							<TreeViewItem Header="jEUR/jUSD" />
							<TreeViewItem Header="jCHF/jGBP" />
						</TreeViewItem>
					</TreeView>
				</Panel>
			</Grid>
		</TabItem>
	</TabControl>
</UserControl>
